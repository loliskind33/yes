
		game:GetService("StarterGui"):SetCore("SendNotification",{
    Title = "sigma peel xds";
    Text = "peels btools made by TrickSter, Lqigma, spigma";
})

if game.PlaceId == 9043532917 then
	local tool = Instance.new("Tool", game.Players.LocalPlayer.Backpack)
		tool.Name = "Delete"
		tool.RequiresHandle = false
		tool.CanBeDropped = false
		tool.TextureId = 'http://www.roblox.com/asset/?id=12223874'
		tool.Equipped:Connect(function(mouse)
			mouse.Button1Down:connect(function()
				if mouse.Target and mouse.Target.Parent then
					game:GetService("ReplicatedStorage").DeleteCar:FireServer(mouse.Target)
				end
			end)
		end)
end
